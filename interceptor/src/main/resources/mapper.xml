<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.seedit.interceptor">
	<select id="loginCheck" parameterMap="hashmap" resultType="int">
		<![CDATA[
			select count(*) from users where user_id=#{userId} and pwd=#{pwd}
		]]>
	</select>
  </mapper>